{
  "tabWidth": 2,
  "useTabs": false,
  "jsxBracketSameLine": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "jsxTrimWhiteSpace": true,
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrder": ["^firebase*", ".css$", "<THIRD_PARTY_MODULES>"],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
